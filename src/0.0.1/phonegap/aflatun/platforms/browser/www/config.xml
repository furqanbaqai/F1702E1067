<?xml version='1.0' encoding='utf-8'?>
<widget id="pk.edu.vu.aflatun" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Aflatun</name>
    <description>
        In-depth AI based analytics of the news
    </description>
    <author email="baqai.furqan@gmail.com" href="https://github.com/furqanbaqai">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="http://*:*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <preference name="FullScreen" value="false" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
